<!--
	测试点列表
	1.测试外键为自身的:单一主键、组合主键、单一cluster primary key、组合cluster primary key
-->
<SQLTEST>
	<CONTENT>  </CONTENT><!-- 请在这里加入对脚本的简要描述 -->
	<CONNECT></CONNECT><!-- 连接数据库 -->
	<TestPointBegin> 测试点1：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T1_INSERT0903 CASCADE; </SQL>
		<SQL> DROP TABLE T2_INSERT0903 CASCADE; </SQL>
		<SQL> DROP TABLE T3_INSERT0903 CASCADE; </SQL>
		<SQL> DROP TABLE T4_INSERT0903 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> CREATE TABLE T1_INSERT0903(C1 INT PRIMARY KEY,FOREIGN KEY(C1) REFERENCES T1_INSERT0903(C1)); </SQL>
		<SQL> CREATE TABLE T2_INSERT0903(C1 INT,C2 INT,PRIMARY KEY(C1,C2),FOREIGN KEY(C1,C2) REFERENCES T2_INSERT0903(C1,C2)); </SQL>
		<SQL> CREATE TABLE T3_INSERT0903(C1 INT CLUSTER PRIMARY KEY,FOREIGN KEY(C1) REFERENCES T3_INSERT0903(C1)); </SQL>
		<SQL> CREATE TABLE T4_INSERT0903(C1 INT,C2 INT,CLUSTER PRIMARY KEY(C1,C2),FOREIGN KEY(C1,C2) REFERENCES T4_INSERT0903(C1,C2)); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> INSERT INTO T1_INSERT0903 VALUES(NULL); </SQL>
		<SQL> INSERT INTO T2_INSERT0903 VALUES(NULL,NULL); </SQL>
		<SQL> INSERT INTO T3_INSERT0903 VALUES(NULL); </SQL>
		<SQL> INSERT INTO T4_INSERT0903 VALUES(NULL,NULL); </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> INSERT INTO T1_INSERT0903 VALUES(1); </SQL>
		<SQL> INSERT INTO T2_INSERT0903 VALUES(1,2); </SQL>
		<SQL> INSERT INTO T3_INSERT0903 VALUES(1); </SQL>
		<SQL> INSERT INTO T4_INSERT0903 VALUES(1,2); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> UPDATE T1_INSERT0903 SET C1 = NULL; </SQL>
		<SQL> UPDATE T2_INSERT0903 SET C2 = NULL; </SQL>
		<SQL> UPDATE T3_INSERT0903 SET C1 = NULL; </SQL>
		<SQL> UPDATE T4_INSERT0903 SET C2 = NULL; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> UPDATE T1_INSERT0903 SET C1 = 999; </SQL>
		<SQL> UPDATE T2_INSERT0903 SET C2 = 999; </SQL>
		<SQL> UPDATE T2_INSERT0903 SET C2 = 777 , C1 = 888; </SQL>
		<SQL> UPDATE T3_INSERT0903 SET C1 = 999; </SQL>
		<SQL> UPDATE T4_INSERT0903 SET C2 = 999; </SQL>
		<SQL> UPDATE T4_INSERT0903 SET C2 = 777 , C1 = 888; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> DELETE FROM T1_INSERT0903; </SQL>
		<SQL> DELETE FROM T2_INSERT0903; </SQL>
		<SQL> DELETE FROM T3_INSERT0903; </SQL>
		<SQL> DELETE FROM T4_INSERT0903; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> INSERT INTO T1_INSERT0903 VALUES(1); </SQL>
		<SQL> INSERT INTO T2_INSERT0903 VALUES(1,2); </SQL>
		<SQL> INSERT INTO T3_INSERT0903 VALUES(1); </SQL>
		<SQL> INSERT INTO T4_INSERT0903 VALUES(1,2); </SQL>
		
	</SQL_CASE>
	<CLEAR><!-- 脚本清除模块，该关键字标识清除块开始 -->
