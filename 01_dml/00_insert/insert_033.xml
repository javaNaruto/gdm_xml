<SQLTEST>
	<CONTENT>  </CONTENT><!-- 请在这里加入对脚本的简要描述 -->
	<CONNECT></CONNECT><!-- 连接数据库 -->
	<TestPointBegin> 测试点1：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> drop table t1_insert0206 cascade; </SQL>
		<SQL> drop table t2_insert0206 cascade; </SQL>
		<SQL> drop table t3_insert0206 cascade; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> create table t1_insert0206(c1 varchar(100),c2 varchar(100)); </SQL>
		<SQL> create table t2_insert0206(c1 int,c2 int); </SQL>
		<SQL> create table t3_insert0206(c1 int,c2 varchar(100),c3 blob); </SQL>

		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> insert into t1_insert0206 values(1,'bb'); </SQL>
		<SQL> insert into t2_insert0206 values(1,2); </SQL>
		<SQL> insert into t3_insert0206 values(1,'aa',0x111); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> insert into t1_insert0206(c2) select * from t2_insert0206; </SQL>
		<SQL> insert into t1_insert0206 select * from t3_insert0206; </SQL>
		<SQL> insert into t2_insert0206 select * from t1_insert0206; </SQL>
		<SQL> insert into t2_insert0206 select C1 from t1_insert0206; </SQL>
		<SQL> insert into t3_insert0206(C2,C3) select * from t2_insert0206; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> insert into t1_insert0206 select * from t2_insert0206; </SQL>
		<SQL> insert into t1_insert0206 select * from t2_insert0206 union select * from t2_insert0206; </SQL>
	</SQL_CASE>	
	
	<TestPointBegin> 测试点2：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> insert into t1_insert0206(c2) select c1 from t2_insert0206; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SELECT_COMPARE_RESULT_FULL </TYPE><!-- 预期执行成功，并返回一个结果集，验证全字匹配长度 -->
		<SQL> select * from t1_insert0206; </SQL>
		<RESULT>
			<RECORD>
				<COLUMN>1</COLUMN><COLUMN>bb</COLUMN>
			</RECORD>
			<RECORD>
				<COLUMN>1</COLUMN><COLUMN>2</COLUMN>
			</RECORD>
			<RECORD>
				<COLUMN>1</COLUMN><COLUMN>2</COLUMN>
			</RECORD>
			<RECORD>
				<COLUMN>NULL</COLUMN><COLUMN>1</COLUMN>
			</RECORD>
		</RESULT>
	</SQL_CASE>
	
	<TestPointBegin> 测试点3：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> drop table t4_insert0206 cascade; </SQL>
		<SQL> drop table t5_insert0206 cascade; </SQL>

		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> create table t4_insert0206(c1 int,c2 int); </SQL>
		<SQL> create table t5_insert0206(c1 int not null,c2 int not null); </SQL>
		<SQL> insert into t4_insert0206 values(null,null); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> insert into t5_insert0206 select * from t4_insert0206; </SQL>
		<SQL> insert into t5_insert0206 values((select null),(select null)); </SQL>
		
		
	</SQL_CASE>	
	
	<CLEAR><!-- 脚本清除模块，该关键字标识清除块开始 -->
		<SQL_CASE>
			<SQL> drop table t1_insert0206 cascade; </SQL>
			<SQL> drop table t2_insert0206 cascade; </SQL>
			<SQL> drop table t3_insert0206 cascade; </SQL>
			<SQL> drop table t4_insert0206 cascade; </SQL>
			<SQL> drop table t5_insert0206 cascade; </SQL>
		</SQL_CASE>
	</CLEAR>
</SQLTEST> 