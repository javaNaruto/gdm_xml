			<RECORD>
				<COLUMN>1</COLUMN><COLUMN>NULL</COLUMN>
			</RECORD>
			<RECORD>
				<COLUMN>a</COLUMN><COLUMN>NULL</COLUMN>
			</RECORD>
		</RESULT>
	</SQL_CASE>	
	
