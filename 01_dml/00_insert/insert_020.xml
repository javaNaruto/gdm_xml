<!--
	测试点列表
	1.测试对象[C1]DEFAULT约束表达式无效
	2.建表列定义NOT NULL，默认值为NULL
-->
<SQLTEST>
	<CONTENT>  </CONTENT><!-- 请在这里加入对脚本的简要描述 -->
	<CONNECT></CONNECT><!-- 连接数据库 -->
	<TestPointBegin> 测试点1：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T1_INSERT0020 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> CREATE TABLE T1_INSERT0020(C1 BOOLEAN DEFAULT DATE'1987-9-9',C2 INT); </SQL>
	</SQL_CASE>
	
	<TestPointBegin> 测试点2：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T2_INSERT0020 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> CREATE TABLE T2_INSERT0020(C1 INT NOT NULL DEFAULT NULL); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> INSERT INTO T2_INSERT0020 DEFAULT VALUES; </SQL>
	</SQL_CASE>
	<CLEAR><!-- 脚本清除模块，该关键字标识清除块开始 -->
		<SQL_CASE>
			<SQL> DROP TABLE T1_INSERT0020 CASCADE; </SQL>
			<SQL> DROP TABLE T2_INSERT0020 CASCADE; </SQL>
		</SQL_CASE>
	</CLEAR>
</SQLTEST> 