	<SQL> DROP TABLE T_RANGE; </SQL>
	<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
	<SQL> CREATE TABLE T_RANGE(C1 INT)PARTITION BY RANGE(C1)
