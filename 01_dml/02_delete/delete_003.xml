<!--
	测试点列表
	1.测试目标表中列不存在	
	2.测试重复列名
	3.测试列名 AS 别名	
-->
<SQLTEST>
	<CONTENT>  </CONTENT><!-- 请在这里加入对脚本的简要描述 -->
	<CONNECT></CONNECT><!-- 连接数据库 -->
	<TestPointBegin> 测试点1：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T1_DELETE0003 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> CREATE TABLE T1_DELETE0003(C1 INT); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> DELETE FROM T1_DELETE0003(C1,C2); </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> INSERT INTO T1_DELETE0003 VALUES(1); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> DELETE FROM T1_DELETE0003(C1,C2); </SQL>
	</SQL_CASE>
	
	<TestPointBegin> 测试点2：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T2_DELETE0003 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> CREATE TABLE T2_DELETE0003(C1 INT); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> DELETE FROM T2_DELETE0003(C1,C1); </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> INSERT INTO T2_DELETE0003 VALUES(1); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> DELETE FROM T2_DELETE0003(C1,C1); </SQL>
	</SQL_CASE>
	
	<TestPointBegin> 测试点3：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T3_DELETE0003 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> CREATE TABLE T3_DELETE0003(C1 INT,C2 VARCHAR(100)); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS _1(C1,C2); </SQL>
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS OLD(C1,C2); </SQL>
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS AUTHORIZATION(C1,C2); </SQL>
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS SELSTAR(C1,C2); </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> INSERT INTO T3_DELETE0003 VALUES(1,2); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS _1(C3,C4); </SQL>
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS OLD(C3,C4); </SQL>
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS AUTHORIZATION(C3,C4); </SQL>
		<SQL> DELETE FROM T3_DELETE0003(C1,C2) AS SELSTAR(C3,C4); </SQL>
	</SQL_CASE>
	<CLEAR><!-- 脚本清除模块，该关键字标识清除块开始 -->
		<SQL_CASE>
			<SQL> DROP TABLE T1_DELETE0003 CASCADE; </SQL>
			<SQL> DROP TABLE T2_DELETE0003 CASCADE; </SQL>
			<SQL> DROP TABLE T3_DELETE0003 CASCADE; </SQL>
		</SQL_CASE>
	</CLEAR>
</SQLTEST> 