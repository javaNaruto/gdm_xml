<!--
	测试点列表
	1.IDENT_SEED ('tablename') 		
-->
<SQLTEST>
	<CONTENT>  </CONTENT><!-- 请在这里加入对脚本的简要描述 -->
	<CONNECT></CONNECT><!-- 连接数据库 -->
	<TestPointBegin> 测试点1：开始 </TestPointBegin>
	<SQL_CASE>
		<TYPE> DIRECT_EXECUTE_IGNORE </TYPE><!-- 不关心语句执行的结果 -->
		<SQL> DROP TABLE T1_IDENTITY1000 CASCADE; </SQL>
		<SQL> DROP TABLE T2_IDENTITY1000 CASCADE; </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> CREATE TABLE T1_IDENTITY1000(C1 INT IDENTITY(1,1)); </SQL>
		<SQL> CREATE TABLE T2_IDENTITY1000(C1 INT); </SQL>
		
		<TYPE> DIRECT_EXECUTE_FAIL </TYPE><!-- 预期执行失败 -->
		<SQL> SELECT IDENT_SEED(?); </SQL>
		<SQL> SELECT IDENT_SEED(&apos;&apos;); </SQL>
		<SQL> SELECT IDENT_SEED(&apos;      &apos;); </SQL>
		<SQL> SELECT IDENT_SEED(&apos;T2_IDENTITY1000&apos;); </SQL>
		
		<TYPE> DIRECT_EXECUTE_SUCCESS </TYPE><!-- 预期执行成功 -->
		<SQL> SELECT IDENT_SEED(NULL); </SQL>
		
		<TYPE> DIRECT_EXECUTE_SELECT_COMPARE_RESULT_FULL </TYPE><!-- 预期执行成功，并返回一个结果集，验证全字匹配长度 -->
		<SQL> SELECT IDENT_SEED(&apos;T1_IDENTITY1000&apos;); </SQL>
		<RESULT>
			<RECORD>
				<COLUMN>1</COLUMN>
			</RECORD>
		</RESULT>
	</SQL_CASE>
	<CLEAR><!-- 脚本清除模块，该关键字标识清除块开始 -->
		<SQL_CASE>
			<SQL> DROP TABLE T1_IDENTITY1000 CASCADE; </SQL>
			<SQL> DROP TABLE T2_IDENTITY1000 CASCADE; </SQL>
		</SQL_CASE>
	</CLEAR>
</SQLTEST> 